.new_pop{
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.8);
  position: fixed;
  z-index: 10;
  left: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.new_pop>view{
  width: 80%;
  height: 60vh;
}
.new_pop>view>image{
  width: 100%;
  height: 100%;
}

