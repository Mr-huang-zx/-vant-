/* 文字爬楼 */
.scroll-box {
  overflow: hidden;
  width: 100%;
}

.item-box {
  overflow: hidden;
  position: relative;
  height: 40rpx;
  /*根据需要设置合适的边距，左边距88rpx是因为左侧有个小图标*/
  /* margin: 26rpx 28rpx 0 88rpx; */
}
.scroll {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
}

.item {
  height: 20px;
  line-height: 40rpx;
  font-size: 30rpx;
  text-align: center;
}
.scroll.trans {
  transition: all 0.24s;
}
